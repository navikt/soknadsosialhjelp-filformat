{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"title": "Sivilstatus og informasjon om eventuell ektefelle.",
	"description": "Hvis \"sivilstatus\" mangler betyr dette at søker ikke har svart på spørsmålet.",
	"type": "object",
	"javaType": "no.nav.sbl.soknadsosialhjelp.soknad.familie.Sivilstatus",
	"properties": {
		"kilde": {
			"$ref": "../parts/kilde.json"
		},
		"status": {
			"title": "Sivilstatus til søker.",
			"type": "string",
			"enum": [
				"gift",
				"ugift",
				"samboer",
				"enke",
				"skilt",
				"separert"
			]
		},
		"ektefelle": {
			"title": "Søkers ektefelle",
			"type": "object",
			"properties": {
				"navn": {
					"$ref": "../parts/navn.json"
				},
				"fodselsdato": {
					"$ref": "../types/dato.json"
				},
				"personIdentifikator": {
					"$ref": "../parts/personIdentifikatorVerdi.json"
				}
			},
			"required": [
				"navn"
			]
		},
		"folkeregistrertMedEktefelle": {
			"description": "Feltet vil kun bli satt når man kan innhente informasjon om dette fra system",
			"type": "boolean"
		},
		"borSammenMed": {
			"type": "boolean"
		},
		"borIkkeSammenMedBegrunnelse": {
			"type": "string"
		}
	},
	"required": [
		"kilde",
		"status"
	],
	"dependencies": {
		"gift": {
			"required": [
				"ektefelle"
			]
		},
		"ektefelle": {
			"properties": {
				"status": {
					"enum": [
						"gift"
					]
				}
			}
		},
		"folkeregistrertMedEktefelle": {
			"properties": {
				"kilde": {
					"enum": [
						"system"
					]
				}
			}
		},
		"borSammenMed": {
			"properties": {
				"kilde": {
					"enum": [
						"bruker"
					]
				}
			}
		},
		"borIkkeSammenMedBegrunnelse": {
			"properties": {
				"kilde": {
					"enum": [
						"bruker"
					]
				}
			}
		}
	}
}